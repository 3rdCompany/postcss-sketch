{
  "name": "postcss-sketch",
  "version": "1.0.0",
  "description": "Playing with new sketch format...",
  "main": "lib/index.js",
  "repository": "git@github.com:3rdCompany/postcss-sketch.git",
  "author": "James Turle <james@foyerlive.com>",
  "license": "MIT",
  "dependencies": {
    "md5": "^2.2.1",
    "null-loader": "^0.1.1",
    "postcss-calc": "^6.0.0",
    "postcss-cli": "^4.1.0",
    "postcss-custom-properties": "^6.0.1",
    "webpack": "^2.6.1"
  },
  "scripts": {
    "build": "babel -d lib/ src/",
    "build:watch": "babel -w -d lib/ src/",
    "test": "postcss test/test.css -o ./test/output.css",
    "start": "",
    "dev": "webpack-dev-server --hot"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1",
      "react"
    ],
    "env": {
      "development": {
        "plugins": [
          [
            "react-transform",
            {
              "transforms": [
                {
                  "transform": "react-transform-hmr",
                  "imports": [
                    "react"
                  ],
                  "locals": [
                    "module"
                  ]
                }
              ]
            }
          ]
        ]
      }
    }
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "postcss-loader": "^2.0.6",
    "postcss-selector-parser": "^2.2.3",
    "postcss-value-parser": "^3.3.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-transform-hmr": "^1.0.4",
    "style-loader": "^0.18.2",
    "watch": "^1.0.2",
    "watchfile-webpack-plugin": "^0.0.4",
    "webpack-dev-server": "2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
